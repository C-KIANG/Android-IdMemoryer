<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="okhttp.com.android.Idmemoryer">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_launch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_launch"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="okhttp.com.android.Idmemoryer.MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="okhttp.com.android.Idmemoryer.RegisterActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="okhttp.com.android.Idmemoryer.Carousel"
            android:label=""
            android:theme="@style/Theme.AppCompat.NoActionBar"
            />
        <activity
            android:name="okhttp.com.android.Idmemoryer.ResetPwd1Activity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="okhttp.com.android.Idmemoryer.ResetPwd2Activity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="okhttp.com.android.Idmemoryer.SelectActivity"
            android:label=""
            android:theme="@style/Theme.AppCompat.NoActionBar" />

    </application>

</manifest>